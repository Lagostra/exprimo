{
  "device_graph_path": "device_graphs/malvik-4gpu.json",
  "net_path": "nets/resnet50.json",
  "optimizer": "map_elites",

  "plot_event_trace": true,
  "event_trace_save_path": "event_trace.png",

  "optimizer_args": {
    "dimension_sizes": [-1, -1, 40],
    "initial_size": 3,
    "mutation_rate": 0.4,
    "copy_mutation_rate": 0.4,
    "replace_mutation_rate": 0.01,
    "crossover_rate": 0.4,
    "steps": 25000,

    "simulator_comp_penalty": 0.9,
    "simulator_comm_penalty": 0.25,
    "allow_cpu": true,
    "pipeline_batches": 1,
    "batches": 1,

    "include_trivial_solutions": false,

    "show_score_plot": true,
    "plot_save_path": "~/map-elites_score_plot.png",
    "plot_axes": [0, 1, 2],

    "verbose": 50,
    "n_threads": -1
  }
}